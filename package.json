{
  "name": "suo-kif",
  "displayName": "SUO-KIF Support",
  "icon": "extension-icon.svg",
  "description": "Syntax highlighting for Standard Upper Ontology Knowledge Interchange Format",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onLanguage:suo-kif"
  ],
  "contributes": {
    "commands": [
      {
        "command": "suo-kif.searchSymbol",
        "title": "Search Symbol in Workspace"
      },
      {
        "command": "suo-kif.showTaxonomy",
        "title": "Show Class Taxonomy"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "suo-kif.searchSymbol",
          "when": "editorLangId == suo-kif",
          "group": "navigation"
        },
        {
          "command": "suo-kif.showTaxonomy",
          "when": "editorLangId == suo-kif",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "SUO-KIF",
      "properties": {
        "suo-kif.language": {
          "type": "string",
          "default": "EnglishLanguage",
          "description": "The language to use for documentation strings (e.g. EnglishLanguage)."
        }
      }
    },
    "languages": [
      {
        "id": "suo-kif",
        "aliases": [
          "SUO-KIF",
          "KIF"
        ],
        "icon": {
            "light": "./kif-file-icon.svg",
            "dark": "./kif-file-icon.svg"
        },
        "extensions": [
          ".kif"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "suo-kif",
        "scopeName": "source.suo-kif",
        "path": "./syntaxes/suo-kif.tmLanguage.json"
      }
    ]
  }
}

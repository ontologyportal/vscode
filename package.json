{
  "name": "suo-kif",
  "displayName": "SUO-KIF Support",
  "description": "Syntax highlighting for Standard Upper Ontology Knowledge Interchange Format",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "suo-kif",
        "aliases": [
          "SUO-KIF",
          "KIF"
        ],
        "extensions": [
          ".kif"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "suo-kif",
        "scopeName": "source.suo-kif",
        "path": "./syntaxes/suo-kif.tmLanguage.json"
      }
    ]
  }
}
